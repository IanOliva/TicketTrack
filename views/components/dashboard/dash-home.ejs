    <div class="title">
        <i class="fa-solid fa-chart-simple"></i>
        <span class="text">Total Administradores</span>
    </div>
    <!-- esta box es para la cantidad de tickets -->
    <div class="boxes">
        <div class="box box1">
            <i class="fa-solid fa-crown"></i>
            <span class="text">Admin</span>

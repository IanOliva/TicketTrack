<body>
    <div class="activity">
        <div class="title">
            <i class="fa-regular fa-clock"></i>
            <span class="text">Recientes</span>

        </div>
        <div class="search-box">
            <i class="fa-solid fa-magnifying-glass"></i>
            <input type="text" placeholder="Buscar..." />
        </div>

        <table class="dash-table">
            <thead>
                <tr>
                    <th>Titulo</th>
                    <th>Descripci√≥n</th>
                    <th>Prioridad</th>
                    <th>Fecha</th>
                    <th>Estado</th>
                    <th>Accion</th>
                </tr>
            </thead>
            <tbody id="table-body">

                <% data.tickets.forEach((t)=> { %>
                    <tr>
                        <td>
                            <%= t.motivo %>
                        </td>
                        <td>
                            <%= t.descripcion %>
                        </td>
                        <td>
                            <%= t.prioridad %>
                        </td>
                        <td>
                            <%= t.fechaOpen %>
                        </td>
                        <td>
                            <%= t.estate %>
                        </td>
                        <td>
                            <a href=""><i class="fa-solid fa-circle-check"></i></a>
                            <a href=""><i class="fa-regular fa-trash-can"></i></a>
                            <a href=""><i class="fa-solid fa-circle-xmark"></i></a>
                        </td>
                    </tr>
                    <% }); %>

            </tbody>
        </table>

    </div>

</body>

</html>